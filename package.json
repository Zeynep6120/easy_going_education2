{
  "name": "modul33-project",
  "version": "1.0.0",
  "description": "Simple JS + SCSS + Express + PostgreSQL starter",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build-sass": "sass scss/style.scss:public/css/main.css --no-source-map",
